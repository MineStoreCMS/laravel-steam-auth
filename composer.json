{
    "name": "teh9/laravel-steam-auth",
    "description": "Laravel Steam Auth",
    "keywords": ["steam", "laravel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Markus Kleiton (fork for Nikita Brytkov repository)"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^6.3|^7.0.1",
        "illuminate/http": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Teh9\\LaravelSteamAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teh9\\LaravelSteamAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Teh9\\LaravelSteamAuth\\SteamServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
